{
  "name": "psd2html",
  "version": "5.0.0",
  "license": "MIT",
  "scripts": {
    "start": "gulp",
    "build": "gulp build",
    "lint:js": "eslint src",
    "lint:js:fix": "eslint src --fix",
    "lint:scss": "stylelint src",
    "lint:scss:fix": "stylelint src --fix",
    "lint:html": "java -jar node_modules/vnu-jar/build/dist/vnu.jar --skip-non-html --errors-only dist",
    "lint:html:full": "java -jar node_modules/vnu-jar/build/dist/vnu.jar --skip-non-html dist"
  },
  "config": {
    "src": {
      "base": "src",
      "pages": "."
    },
    "dist": {
      "base": "dist",
      "assets": "assets"
    }
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "buffer-replace": "^1.0.0",
    "colors": "^1.3.3",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cache": "^1.1.1",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^5.0.0",
    "gulp-html-prettify": "^0.0.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-insert": "^0.5.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-tap": "^1.0.1",
    "gulp-twig": "https://github.com/maxvien/gulp-twig/archive/v1.3.0.tar.gz",
    "gulp-watch": "^5.0.1",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.5.4",
    "vnu-jar": "latest"
  }
}
